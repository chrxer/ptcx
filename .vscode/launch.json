    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger:",
            "type": "debugpy",
            "request": "launch",
            "module": "ptcx"
        },
        {
            "name": "Python Debugger: reset",
            "type": "debugpy",
            "request": "launch",
            "module": "ptcx",
            "args": ["--reset"]
        },
        {
            "name": "Build",
            "type": "debugpy",
            "request": "launch",
            "module": "build"
        },
        {
            "name": "mkdocs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mkdocs.py"
        },
    ]
}